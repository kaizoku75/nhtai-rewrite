{
    "name": "nhtai_rewriter",
    "version": "2.3.2",
    "description": "nHtai Rewrite.",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "postinstall": "webpack -p --mode production",
        "webpack:dev": "webpack-dev-server --mode development --open --port $PORT --host $IP --public $C9_HOSTNAME",
        "dev:server": "nodemon index.js",
        "dev:static": "webpack --mode development --watch",
        "build": "webpack -p --mode production",
        "test": "jest",
        "test:watch": "jest --watchAll",
        "lint": "eslint --ext .js --ext .jsx src"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js"
        },
        "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests"
    },
    "devDependencies": {
        "babel-core": "6.26.3",
        "babel-eslint": "10.0.1",
        "babel-loader": "7.1.5",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-preset-env": "1.7.0",
        "babel-preset-react": "6.24.1",
        "clean-webpack-plugin": "0.1.19",
        "css-loader": "3.0.0",
        "enzyme": "3.10.0",
        "enzyme-adapter-react-16": "1.14.0",
        "eslint": "5.16.0",
        "eslint-plugin-jest": "21.27.2",
        "eslint-plugin-react": "7.13.0",
        "extract-text-webpack-plugin": "4.0.0-beta.0",
        "file-loader": "4.0.0",
        "html-webpack-plugin": "3.2.0",
        "image-webpack-loader": "5.0.0",
        "jest": "23.6.0",
        "moxios": "0.4.0",
        "script-ext-html-webpack-plugin": "2.1.3",
        "style-loader": "0.23.1",
        "url-loader": "2.0.0",
        "webpack": "4.33.0",
        "webpack-cli": "3.3.3",
        "webpack-dev-server": "3.7.1"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "compression": "^1.7.3",
        "disqus-react": "*",
        "express": "^4.16.3",
        "jquery": "^3.4.0",
        "lodash": "^4.17.10",
        "prop-types": "^15.6.2",
        "react": "^16.5.0",
        "react-dom": "^16.5.0",
        "react-ga": "^2.5.7",
        "react-helmet": "^5.2.0",
        "react-lazyload": "^2.3.0",
        "react-load-image": "^0.1.6",
        "react-redux": "^7.0.0",
        "react-router-dom": "^4.3.1",
        "redux": "^4.0.0",
        "redux-promise": "^0.6.0",
        "request": "^2.88.0"
    }
}
